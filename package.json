{
	"name": "breaking-bad-quotes",
	"displayName": "Breaking Bad quotes",
	"description": "Random quotes from the legendary series: Breaking Bad by Vince Gilligan",
	"publisher": "okay-head",
	"repository": {
		"url": "https://github.com/okay-head/BreakingBad-vscode-extension"
	},
	"icon": "https://raw.githubusercontent.com/okay-head/BreakingBad-vscode-extension/main/extension-icon.png",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.89.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "breaking-bad-quotes.insertRandomQuote",
				"title": "Insert a Quote from Breaking Bad"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"@types/vscode": "^1.89.0",
		"@typescript-eslint/eslint-plugin": "^7.7.1",
		"@typescript-eslint/parser": "^7.7.1",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.3.9",
		"eslint": "^8.57.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"webpack": "^5.91.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"axios": "^1.7.2"
	}
}
